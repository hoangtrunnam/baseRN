{
  "name": "baseRN",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "react-native run-android --mode=devDebug",
    "android:staging": "react-native run-android --mode=stagingDebug",
    "android:prod": "react-native run-android --mode=productDebug",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "prepare": "husky install",
    "type": "tsc --noEmit --composite false",
    "lint": "eslint --quiet \"**/*.{js,ts,tsx}\"",
    "checklint": "yarn lint && yarn type",
    "lint:fix": "eslint --quiet \"**/*.{js,ts,tsx}\" --fix",
    "podinstall": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "@alessiocancian/react-native-actionsheet": "^3.2.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-clipboard/clipboard": "^1.15.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/material-top-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "i18n-js": "3.9.2",
    "i18next": "^23.5.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.46",
    "react": "18.2.0",
    "react-i18next": "^13.2.2",
    "react-native": "0.73.1",
    "react-native-blob-util": "^0.19.11",
    "react-native-calendars": "^1.1307.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^14.0.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-keychain": "^9.2.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "3.3.0",
    "react-native-modalize": "^2.1.1",
    "react-native-pager-view": "^6.2.1",
    "react-native-permissions": "^3.9.3",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "^3.16.6",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^13.14.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-webview": "^13.6.2",
    "react-redux": "^8.1.3",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@tsconfig/react-native": "^3.0.0",
    "@types/i18n-js": "3.8.2",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "detox": "^20.13.5",
    "eslint": "^8.51.0",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "test",
          "revert",
          "add",
          "chore",
          "build"
        ]
      ]
    }
  }
}
